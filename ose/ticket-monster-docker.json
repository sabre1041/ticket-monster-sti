{
    "kind": "Config",
    "apiVersion": "v1beta1",
    "metadata": {
        "creationTimestamp": null
    },
    "items": [
        {
            "kind": "Service",
            "id": "tm-docker-http-service",
            "creationTimestamp": null,
            "apiVersion": "v1beta1",
            "annotations": {
                "description": "The web server's http port."
            },
            "port": 8080,
            "protocol": "TCP",
            "containerPort": 8080,
            "labels": {
                "application": "tm-docker",
                "template": "eap6-basic-sti"
            },
            "selector": {
                "deploymentConfig": "tm-docker"
            },
            "sessionAffinity": "None",
            "ports": [
                {
                    "name": "",
                    "protocol": "TCP",
                    "port": 8080,
                    "containerPort": 8080
                }
            ]
        },
        {
            "kind": "Service",
            "id": "tm-docker-ping",
            "creationTimestamp": null,
            "apiVersion": "v1beta1",
            "annotations": {
                "description": "Ping service for clustered applications."
            },
            "port": 8888,
            "protocol": "TCP",
            "containerPort": 8888,
            "labels": {
                "application": "tm-docker",
                "template": "eap6-basic-sti"
            },
            "selector": {
                "deploymentConfig": "tm-docker"
            },
            "portalIP": "None",
            "sessionAffinity": "None",
            "ports": [
                {
                    "name": "",
                    "protocol": "TCP",
                    "port": 8888,
                    "containerPort": 8888
                }
            ]
        },
        {
            "kind": "Route",
            "apiVersion": "v1beta1",
            "metadata": {
                "name": "tm-http-route",
                "creationTimestamp": null,
                "labels": {
                    "application": "tm-docker",
                    "template": "eap6-basic-sti"
                },
                "annotations": {
                    "description": "Route for application's http service."
                }
            },
            "host": "ticketmonster-docker.ose.example.com",
            "serviceName": "tm-docker-http-service"
        },
        {
            "kind": "ImageStream",
            "apiVersion": "v1beta1",
            "metadata": {
                "name": "tm-docker",
                "creationTimestamp": null,
                "labels": {
                    "application": "tm-docker",
                    "template": "eap6-basic-sti"
                }
            },
            "spec": {},
            "status": {
                "dockerImageRepository": ""
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "v1beta1",
            "metadata": {
                "name": "tm-docker",
                "creationTimestamp": null,
                "labels": {
                    "application": "tm-docker",
                    "template": "eap6-basic-sti"
                }
            },
            "triggers": [
                {
                    "type": "generic",
                    "generic": {
                        "secret": "secret"
                    }
                }
            ],
            "parameters": {
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/sabre1041/ticket-monster-sti",
                        "ref": "ose3-docker"
                    },
                    "contextDir": "demo"
                },
                "strategy": {
                    "type": "Docker",
                    "dockerStrategy": {
                        "contextDir": "demo",
						"tag": "6.4",
                        "from": {
                            "kind": "ImageStream",
                            "name": "jboss-eap6-openshift",
							"namespace": "openshift"
                        },
                        "clean": true
                    }
                },
                "output": {
                    "to": {
                        "name": "tm-docker"
                    }
                }
            }
        },
        {
            "kind": "DeploymentConfig",
            "apiVersion": "v1beta1",
            "metadata": {
                "name": "tm-docker",
                "creationTimestamp": null,
                "labels": {
                    "application": "tm-docker",
                    "template": "eap6-basic-sti"
                }
            },
            "triggers": [
                {
                    "type": "ImageChange",
                    "imageChangeParams": {
                        "automatic": true,
                        "containerNames": [
                            "tm-docker"
                        ],
                        "from": {
                            "name": "tm-docker"
                        },
                        "tag": "latest",
                        "lastTriggeredImage": ""
                    }
                }
            ],
            "template": {
                "strategy": {
                    "type": "Recreate"
                },
                "controllerTemplate": {
                    "replicas": 1,
                    "replicaSelector": {
                        "deploymentConfig": "tm-docker"
                    },
                    "podTemplate": {
                        "desiredState": {
                            "manifest": {
                                "version": "v1beta2",
                                "id": "",
                                "volumes": null,
                                "containers": [
                                    {
                                        "name": "tm-docker",
                                        "image": "tm-docker",
                                        "ports": [
                                            {
                                                "name": "tm-docker-tcp-8080",
                                                "containerPort": 8080,
                                                "protocol": "TCP"
                                            },
                                            {
                                                "name": "tm-docker-ping-8888",
                                                "containerPort": 8888,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "env": [
                                            {
                                                "name": "OPENSHIFT_PING_SERVICE",
                                                "key": "OPENSHIFT_PING_SERVICE",
                                                "value": "eap-app-ping"
                                            },
                                            {
                                                "name": "OPENSHIFT_PING_NAMESPACE",
                                                "key": "OPENSHIFT_PING_NAMESPACE"
                                            },
                                            {
                                                "name": "OPENSHIFT_PING_DOMAIN",
                                                "key": "OPENSHIFT_PING_DOMAIN",
                                                "value": "local"
                                            }
                                        ],
                                        "resources": {},
                                        "terminationMessagePath": "/dev/termination-log",
                                        "imagePullPolicy": "PullAlways",
                                        "capabilities": {}
                                    }
                                ],
                                "restartPolicy": {
                                    "always": {}
                                },
                                "dnsPolicy": "ClusterFirst"
                            }
                        },
                        "labels": {
                            "application": "tm-docker",
                            "deploymentConfig": "tm-docker",
                            "template": "eap6-basic-sti"
                        }
                    }
                }
            }
        }
    ]
}
